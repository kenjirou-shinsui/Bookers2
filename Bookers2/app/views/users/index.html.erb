<div class="container">
     <div class="row">  

         <div class="col-xs-3">

<%= render 'books/list', {bookx: @bookx, user: current_user} %>
</div>
<div class="col-xs-9">
<h1>Users</h1>
<table class="table table-hover table-inverse">

  	<thead>
  		<tr>
    <th>image</th>
    <th>name</th>
    <th></th>
  </tr>
  </thead>
  <% @users.each do |user| %>
  <tr>
  <td>
<%= attachment_image_tag user, :profile_image, :fill, 60, 60, fallback: "no_image.jpg",size:'60x60' %></td>
<td>
	<%= user.name %></td>
	<td>
		<%= link_to "Show" ,user_path(user.id) %>
	</td>
</tr>

<% end %>
</table>
</div>
</div>
</div>